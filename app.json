{
  "expo": {
    "name": "Family App Management System",
    "slug": "family-app-management-system",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scripts": {
      "start": "bunx rork start -p edd19ns3buegzug8rrdb6 --tunnel",
      "start-web": "bunx rork start -p edd19ns3buegzug8rrdb6 --web --tunnel",
      "start-web-dev": "DEBUG=expo* bunx rork start -p edd19ns3buegzug8rrdb6 --web --tunnel",
      "start:tunnel": "npx expo start --clear --tunnel",
      "start:lan": "npx expo start --clear --lan",
      "clean:locks": "rm -f package-lock.json yarn.lock pnpm-lock.yaml",
      "clean:expo": "rm -rf .expo .expo-shared dist web-build",
      "clean:caches": "rm -rf node_modules/.cache .cache",
      "clean:modules": "rm -rf node_modules && bun install",
      "clean": "bun run clean:locks && bun run clean:expo && bun run clean:caches",
      "reset": "bun run clean && bun run doctor && bun run deps:check",
      "doctor": "npx expo-doctor@latest",
      "deps:check": "npx expo install --check",
      "lint": "expo lint"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "app.rork.family-app-management-system",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow $(PRODUCT_NAME) to access your photos",
        "NSCameraUsageDescription": "Allow $(PRODUCT_NAME) to access your camera",
        "NSMicrophoneUsageDescription": "Allow $(PRODUCT_NAME) to access your microphone",
        "NSFaceIDUsageDescription": "Allow $(PRODUCT_NAME) to use Face ID.",
        "NSCalendarsUsageDescription": "Allow $(PRODUCT_NAME) to access your calendar",
        "NSRemindersUsageDescription": "Allow $(PRODUCT_NAME) to access your reminders",
        "NSPhotoLibraryAddUsageDescription": "Allow $(PRODUCT_NAME) to save photos."
      },
      "entitlements": {
        "com.apple.developer.networking.wifi-info": true,
        "com": {
          "apple": {
            "developer": {
              "networking": {
                "wifi-info": true
              }
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rork.familyapp",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "READ_CALENDAR",
        "WRITE_CALENDAR",
        "INTERNET",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.SCHEDULE_EXACT_ALARM",
        "android.permission.REQUEST_INSTALL_PACKAGES",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "READ_MEDIA_AUDIO",
        "READ_MEDIA_VISUAL_USER_SELECTED",
        "ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      [
        "expo-calendar",
        {
          "calendarPermission": "The app needs to access your calendar."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./local/assets/notification_icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./local/assets/notification_sound.wav"
          ],
          "enableBackgroundRemoteNotifications": false
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    }
  }
}